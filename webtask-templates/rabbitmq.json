{
    "title": "Auth0 Logs to RABBITMQ",
    "name": "auth0-logs-to-RABBITMQ",
    "version": "0.0.1",
    "preVersion": "0.0.0",
    "description": "This extension will export Auth0 to RabbitMQ",
    "secrets": {
      "RABBITMQ_URI_HOSTNAME": {
        "description": "RabbitMQ instance hostname",
        "example": "localhost",
        "required": true
      },
      "RABBITMQ_URI_PROTOCOL": {
        "description": "RabbitMQ protocol",        
        "example": "amqp",
        "required": true
      },
      "RABBITMQ_URI_PORT": {
        "description": "RabbitMQ Port",
        "required": true
      },
      "RABBITMQ_URI_USER": {
        "description": "RabbitMQ User",
        "required": true
      },
      "RABBITMQ_URI_PASSWORD": {
        "description": "RabbitMQ Password",
        "type": "password",
        "required": true
      },
      "RABBITMQ_URI_LOCALE": {
        "description": "RabbitMQ locale",
        "required": true
      },
      "RABBITMQ_URI_FRAMEMAX": {
        "description": "RabbitMQ frameMax",
        "required": true
      },
      "RABBITMQ_URI_HEARTBEAT": {
        "description": "RabbitMQ heartbeat",
        "required": true
      },
      "RABBITMQ_URI_VHOST": {
        "description": "RabbitMQ vhost",
        "required": true
      },
      "RABBITMQ_URI_EXCHANGE": {
        "description": "RabbitMQ exchange",
        "required": true
      },
      "RABBITMQ_URI_ROUTINGKEY": {
        "description": "RabbitMQ routing key"
      }
    }
  }
  